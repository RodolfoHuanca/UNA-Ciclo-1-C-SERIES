#include <iostream>
using namespace std;
int main() {
    int n;
    cin >> n;

    int x = 1;
    int y = 1;

    while (x <= n) {
        for (int i = 0; i < y; ++i) {
            if (x > n) break;
            cout << x << " ";
            x=x+1;
        }
        cout << "0 ";
        y=y+1;
    }
}
