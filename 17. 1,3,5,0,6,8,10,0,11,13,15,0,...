#include <iostream>
using namespace std;
int main() {
    int n;
    cin >> n;

    int x = 1;
    int y = 6;
    int z = 0;

    while (z < n) {
        // Imprimimos tres números impares
        for (int i = 0; i < 3 && z < n; ++i) {
            cout << x << " ";
            x += 2;
            z++;
        }

        // Imprimimos tres números pares
        for (int i = 0; i < 3 && z < n; ++i) {
            cout << y << " ";
            y += 2;
            z++;
        }
    }
}
